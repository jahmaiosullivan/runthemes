<!DOCTYPE html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
<!--[if IE 7 ]><html class="ie ie7" lang="en"> <![endif]-->
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!-->
    <html lang="en"> 
<!--<![endif]-->
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width">
        <meta name="description" content="">
        <title>MvcSkins - @ViewBag.Title</title>

        <!-- Bootstrap core CSS -->
        <link href="~/Content/bootstrap.min.css" rel="stylesheet">
        <!-- Style CSS -->
        <link href="~/Content/style.css" rel="stylesheet">
        <!-- Customize CSS -->
        <link href="~/Content/custom.css" rel="stylesheet">
        <!-- Responsive CSS -->
        <link href="~/Content/responsive.css" rel="stylesheet">
        <!-- Demo CSS -->
        <link href="~/Content/demo.css" rel="stylesheet">
        
        
        <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
      <script src="~/Scripts/html5shiv.js"></script>
      <script src="~/Scripts/respond.min.js"></script>
    <![endif]-->
        <!-- Favicons -->
        <link rel="apple-touch-icon" href="~/Images/apple-touch-icon.png">
        <link rel="apple-touch-icon" sizes="57x57" href="~/Images/apple-touch-icon-57x57.png">
        <link rel="apple-touch-icon" sizes="72x72" href="~/Images/apple-touch-icon-72x72.png">
        <link rel="apple-touch-icon" sizes="114x114" href="~/Images/apple-touch-icon-114x114.png">
        <link rel="apple-touch-icon" sizes="144x144" href="~/Images/apple-touch-icon-144x144.png">
        <style id="fit-vids-style">
            .fluid-width-video-wrapper {
                width: 100%;
                position: relative;
                padding: 0;
            }

                .fluid-width-video-wrapper iframe, .fluid-width-video-wrapper object, .fluid-width-video-wrapper embed {
                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                }
        </style>
        <style type="text/css"></style>
        @RenderSection("styles", required: false)
    </head>
        <body>
            @if (!Request.IsAuthenticated)
            {
                @Html.Partial("LoginModal")
                @Html.Partial("RegisterModal")
            }
            @RenderBody()
            <footer>
                @Html.Partial("Footer")
                @RenderSection("footer", required: false)
            </footer>
            <!--jQuery-->
            <script type="text/javascript" src="/scripts/One/jquery-1.10.2.min.js"></script>
            <!--Parallax-->
            <script type="text/javascript" src="/scripts/One/stellar.js"></script>
            <!--Fancy Select-->
            <script type="text/javascript" src="/scripts/One/fancySelect.js"></script>
            <!--jQuery Ui-->
            <script type="text/javascript" src="/scripts/One/jquery-ui.js"></script>
            <!--Pretty Photo-->
            <script type="text/javascript" src="/scripts/One/jquery.prettyPhoto.js"></script>
            <!--Header Sticky -->
            <script type="text/javascript" src="/scripts/One/waypoints.min.js"></script>
            <!--Mobile Menu -->
            <script type="text/javascript" src="/scripts/One/modernizr.custom.js"></script>
            <script type="text/javascript" src="/scripts/One/jquery.dlmenu.js"></script>
            <!--Owl Carousel-->
            <script type="text/javascript" src="/scripts/One/owl.carousel.js"></script>
            <!--iSotope-->
            <script type="text/javascript" src="/scripts/One/isotope.pkgd.min.js"></script>
            <!--Circle Chart-->
            <script type="text/javascript" src="/scripts/One/jquery.circliful.min.js"></script>
            <!--scrollReveal-->
            <script type="text/javascript" src="/scripts/One/scrollReveal.js"></script>
            <!--Html5 player-->
            <script type="text/javascript" src="/scripts/One/mediaelement-and-player.min.js"></script>
            <!--Countdown-->
            <script type="text/javascript" src="/scripts/One/TimeCircles.js"></script>
            <!--Counter Up-->
            <script type="text/javascript" src="/scripts/One/jquery-counterup.js"></script>
            <!-- Responsive video -->
            <script type="text/javascript" src="/scripts/One/jquery.fitvids.js"></script>
            <!--Flat Surface Shader -->
            <script type="text/javascript" src="/scripts/One/fss.js"></script>
            <!-- SLIDER REVOLUTION 4.x SCRIPTS  -->
            <script type="text/javascript" src="/scripts/One/jquery.themepunch.plugins.min.js"></script>
            <script type="text/javascript" src="/scripts/One/jquery.themepunch.revolution.min.js"></script>
            <!--Onepage scroll-->
            <script type="text/javascript" src="/scripts/One/jQuery-scrollnav.js"></script>
            <!--Background video-->
            <script type="text/javascript" src="/scripts/One/jquery.mb.YTPlayer.js"></script>
            <!--Retina support-->
            <script type="text/javascript" src="/scripts/One/retina.js"></script>
            <!--Main Script-->
            <script type="text/javascript" src="/scripts/One/main.js"></script> <!--Customize Script-->
        
            <script type="text/javascript" src="~/Scripts/One/bootstrap.min.js"></script>
            @RenderSection("scripts", required: false)
        </body>
</html>
