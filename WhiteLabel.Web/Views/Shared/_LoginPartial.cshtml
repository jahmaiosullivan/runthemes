@if (Request.IsAuthenticated)
{
    using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new {id = "logoutForm", @class = "navbar-right"}))
    {
        @Html.AntiForgeryToken()
    }
    <li><a href="#" title="user profile">MyName</a></li>
    <li><a href="javascript:document.getElementById('logoutForm').submit()" class="external">Log out</a></li>
}
else
{
    <li><a id="registerLink" class="external" data-toggle="modal" href="@Url.Action("Register", "Account")" data-target="#myModal">Register</a></li>
    <li><a id="loginLink" class="external" data-toggle="modal" href="@Url.Action("Login", "Account")" data-target="#myModal">Log in</a></li>
}
